{
  "ConnectionStrings": {
    "DefaultConnection": "datasource=backend-db-1;database=kwetter;userid=root;password=example"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "ServerRealm": "http://localhost:8081/auth/realms/Kwetter",
    "Metadata": "http://localhost:8081/auth/realms/Kwetter/.well-known/openid-configuration",
    "ClientId": "someclientname",
    "ClientSecret": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "TokenExchange": "http://localhost:8081/auth/realms/Kwetter/protocol/openid-connect/token",
    "Audience": "some-audience"
  },
    "AllowedHosts": "*"
  }
