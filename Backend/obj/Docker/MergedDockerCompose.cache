services:
  db:
    command: --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_ROOT_PASSWORD: example
    image: mysql
    ports:
    - published: 3306
      target: 3306
    restart: always
  kweet:
    build:
      context: C:\Users\Nickv\Documents\School\Semester 6\individueel\Kwetter-semester-6\Backend
      dockerfile: Kweet/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    image: kweet
    ports:
    - published: 5050
      target: 80
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Nickv\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Nickv\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  phpmyadmin:
    environment:
      PMA_ARBITRARY: '1'
    image: phpmyadmin
    ports:
    - published: 8080
      target: 80
    restart: always
version: '3.4'